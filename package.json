{
  "name": "@ibmgaragecloud/cloud-native-toolkit-cli",
  "version": "0.4.9-beta.0",
  "description": "CLI that provides functions to simplify interactions with containerized platforms and tools",
  "main": "dist/index.js",
  "author": "Sean Sundberg <seansund@us.ibm.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "igc": "dist/script.js"
  },
  "watch": {
    "test": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": [
        "npm test"
      ],
      "after:bump": "npm run build",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    }
  },
  "scripts": {
    "test": "jest --coverage",
    "tdd": "jest --watch",
    "build": "tsc -d",
    "postbuild": "chmod +x dist/script.js",
    "prelink": "npm run build",
    "link": "npm link $npm_package_name",
    "unlink": "npm unlink $npm_package_name",
    "prepare": "npm run build",
    "watch": "npm-watch",
    "prestart": "npm run build",
    "start": "./dist/script.js",
    "igc": "./dist/script.js",
    "release": "release-it --verbose"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/dockerode": "^2.5.19",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.11",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^11.13.2",
    "@types/open": "^6.1.0",
    "@types/ora": "^3.2.0",
    "@types/puppeteer": "^1.12.4",
    "@types/puppeteer-core": "^1.9.0",
    "@types/rimraf": "^2.0.3",
    "@types/superagent": "^4.1.1",
    "@types/tar": "^4.0.3",
    "@types/yargs": "^13.0.0",
    "babel-jest": "^24.8.0",
    "husky": "^3.0.2",
    "jest": "^24.7.1",
    "jest-plugin-context": "^2.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "npm-watch": "^0.6.0",
    "release-it": "^12.6.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "dockerode": "^2.5.8",
    "dot-properties": "^0.3.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^6.4.1",
    "json2yaml": "^1.1.0",
    "jsonpath": "^1.0.2",
    "kubernetes-client": "^8.3.0",
    "lodash": "^4.17.15",
    "open": "^6.3.0",
    "openshift-rest-client": "^3.0.0",
    "ora": "^3.4.0",
    "rimraf": "^3.0.2",
    "superagent": "^5.1.0",
    "tar": "^5.0.5",
    "tslib": "^1.10.0",
    "typescript-ioc": "^1.2.5",
    "yargs": "^13.2.2"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibm-garage-cloud/ibm-garage-cloud-cli.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
